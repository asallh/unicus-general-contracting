generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider  = "postgresql"
  url       = env("DATABASE_URL")
  directUrl = env("DIRECT_URL")
}

model Project {
  id             String   @id @default(cuid())
  title          String
  description    String
  imageURL       String[]
  createdAt      DateTime @default(now())
  updatedAt      DateTime @updatedAt
  featuredOnHome Boolean  @default(false)
}
